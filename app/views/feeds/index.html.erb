<h1>投稿した写真一覧</h1>
<%= link_to "画像を投稿する", new_feed_path, class: "btn btn-primary" %>

<div class="mb-3">
  <table class="table">
    <thead>
      <tr>
        <th>写真</th>
        <th>コメント</th>
      </tr>
    </thead>
    <tbody>
      <% @feeds.each do |feed| %>
        <tr>
          <td><%= link_to feed.image, feed_path(feed.id) %></td>
          <td><%= feed.description %></td>
          <td><%= link_to "編集", edit_feed_path(feed.id), class: "btn btn-primary" %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>