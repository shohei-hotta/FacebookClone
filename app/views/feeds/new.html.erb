<h1>写真の投稿</h1>

<div class="nav justify-content-end">
  <%= link_to "一覧", feeds_path, class: "nav-link" %>
</div>

<%= form_with(model: @feed, local: true) do |f| %>
  <div class="form-group">
    <%= f.label :image %>
    <%= f.text_area :image, class: "form-control", id: "feed_image" %>
  </div>
  <div class="form-group">
    <%= f.label :description %>
    <%= f.text_area :description, class: "form-control", id: "feed_description" %>
  </div>
  <%= f.submit nil, class: "btn btn-primary" %>
<% end %>